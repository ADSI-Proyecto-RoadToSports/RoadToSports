<div id="myTabContent" class="tab-content">
  <div class="tab-pane fade active in cont1" id="inicio">
<h1>Fecha y lugar del partido</h1>
<%= form_tag fch_lg_matches_path, :method => 'get' do %>
<% content_tag :label do %>
<%= text_field_tag :search, params[:search] %>
<%= submit_tag "Buscar", :name => nil %>
<% end %>
<% end %>
<table  class="table table-striped table-hover " id="inicio">
  <thead>
    <tr>
      <th>Fecha de Inicio</th>
      <th>Fecha de Clausura</th>
      <th>Lugar</th>
      <th>Descripci√≥n</th>
      <th>Equipo</th>
      <th>Torneo</th>
      <th colspan="3">Opciones</th>
    </tr>
  </thead>

  <tbody>
    <% @fch_lg_matches.each do |fch_lg_match| %>
      <tr>
        <td><%= fch_lg_match.startdate %></td>
        <td><%= fch_lg_match.endingdate %></td>
        <td><%= fch_lg_match.place %></td>
        <td><%= fch_lg_match.description %></td>
        <td><%= fch_lg_match.team %></td>
        <td><%= fch_lg_match.tournament %></td>
        <td><%= link_to 'Ver', fch_lg_match %></td>
        <td><%= link_to 'Editar', edit_fch_lg_match_path(fch_lg_match) %></td>
        <td><%= link_to 'Eliminar', fch_lg_match, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= button_to 'Nueva fecha', new_fch_lg_match_path, :method=>"get", class: "btn btn-info" %><br>
