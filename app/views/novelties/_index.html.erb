<h1>Listing novelties</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>New type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @novelties.each do |novelty| %>
      <tr>
        <td><%= novelty.name %></td>
        <td><%= novelty.description %></td>
        <td><%= novelty.new_type %></td>
      </tr>
    <% end %>
  </tbody>
</table>

