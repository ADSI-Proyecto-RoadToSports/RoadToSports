<h1>Listing nombrepartidoms</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nombrepartidoms.each do |nombrepartidom| %>
      <tr>
        <td><%= nombrepartidom.nombre %></td>
        <td><%= link_to 'Show', nombrepartidom %></td>
        <td><%= link_to 'Edit', edit_nombrepartidom_path(nombrepartidom) %></td>
        <td><%= link_to 'Destroy', nombrepartidom, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nombrepartidom', new_nombrepartidom_path %>
