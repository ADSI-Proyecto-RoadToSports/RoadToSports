<h1>Listing new_types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @new_types.each do |new_type| %>
      <tr>
        <td><%= new_type.name %></td>
        <td><%= new_type.description %></td>
        <td><%= link_to 'Show', new_type %></td>
        <td><%= link_to 'Edit', edit_new_type_path(new_type) %></td>
        <td><%= link_to 'Destroy', new_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New New type', new_new_type_path %>
