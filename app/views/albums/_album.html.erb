<tr class="mov2" id=<%= "album_#{album.id}"%>>
        <td><%= link_to image_tag(album.image.url(:medium)), album, remote: true, class: "image" %><p class="title"><br><%= album.title %></p> <div class="botones"><%= link_to 'Editar', edit_album_path(album), remote: true, :class => 'btn btn-primary' %>
       <%= link_to 'Emilinar', album, method: :delete, data: { confirm: 'Esta seguro?' }, :class => 'btn btn-danger'%>
       <%= link_to 'Ver imagenes', album_galleries_path(album), :class=>"btn btn-default " %> &nbsp;&nbsp;&nbsp; </div>
        </td>
      </tr>