<h1>Listing type_configurations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_configurations.each do |type_configuration| %>
      <tr>
        <td><%= type_configuration.name %></td>
        <td><%= type_configuration.description %></td>
        <td><%= link_to 'Show', type_configuration %></td>
        <td><%= link_to 'Edit', edit_type_configuration_path(type_configuration) %></td>
        <td><%= link_to 'Destroy', type_configuration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type configuration', new_type_configuration_path %>
