<div class="contenido ">
<div class="img1"></div>
<div class="img2"></div>
<ul class="nav nav-tabs" style="margin-bottom: 15px;">

</ul>
<div id="myTabContent" class="tab-content">
  <div class="tab-pane fade active in cont1" id="inicio">
<h1>Listing matches</h1>

<table>
  <thead>
    <tr>
      <th>Teama</th>
      <th>Teamb</th>
      <th>Modalitie</th>
      <th>Tournament</th>
      <th>Score</th>
      <th>Fchlgmatch</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @matches.each do |match| %>
      <tr>
        <td><%= match.teamA %></td>
        <td><%= match.teamB %></td>
        <td><%= match.modalitie %></td>
        <td><%= match.tournament %></td>
        <td><%= match.score %></td>
        <td><%= match.fchlgmatch %></td>
        <td><%= link_to 'Show', match %></td>
        <td><%= link_to 'Edit', edit_match_path(match) %></td>
        <td><%= link_to 'Destroy', match, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Match', new_match_path %>
