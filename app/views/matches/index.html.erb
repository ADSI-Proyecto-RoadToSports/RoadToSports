<h1>Partidos</h1>
<%= form_tag matches_path, :method => 'get' do %>
<% content_tag :label do %>
<%= text_field_tag :search, params[:search] %>
<%= submit_tag "Buscar", :name => nil %>
<% end %>
<% end %>
<table>
  <thead>
    <tr>
      <th>Equipo A</th>
      <th>Equipo B</th>
      <th>Modalidad</th>
      <th>Torneo</th>
      <th>Puntuaci√≥n</th>
      <th>Fecha lugar Equipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @matches.each do |match| %>
      <tr>
        <td><%= match.teamA %></td>
        <td><%= match.teamB %></td>
        <td><%= match.modalitie %></td>
        <td><%= match.tournament %></td>
        <td><%= match.score %></td>
        <td><%= match.fchlgmatch %></td>
        <td><%= link_to 'Ver', match %></td>
        <td><%= link_to 'Editar', edit_match_path(match) %></td>
        <td><%= link_to 'Eliminar', match, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

