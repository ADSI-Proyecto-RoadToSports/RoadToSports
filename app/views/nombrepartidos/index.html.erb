<h1>Listing nombrepartidos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nombrepartidos.each do |nombrepartido| %>
      <tr>
        <td><%= nombrepartido.nombre %></td>
        <td><%= link_to 'Show', nombrepartido %></td>
        <td><%= link_to 'Edit', edit_nombrepartido_path(nombrepartido) %></td>
        <td><%= link_to 'Destroy', nombrepartido, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

