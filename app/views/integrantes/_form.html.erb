<%= form_for [@team,@integrante] do |f| %>
  <% if @integrante.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@integrante.errors.count, "error") %> prohibited this integrante from being saved:</h2>

      <ul>
      <% @integrante.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :nombre %><br>
    <%= f.text_field :nombre %>
  </div>
  <div class="field">
    <label>Tipo de documento</label><br>
    <%= f.collection_select :documenttype_id, DocumentType.all, :id, :name, class: "form-control", prompt: "Seleccione Un tipo de documento" %>
  </div>
  <div class="field">
    <%= f.label :documento %><br>
    <%= f.text_field :documento %>
  </div>
  <div class="field">
    <%= f.label :ficha %><br>
    <%= f.text_field :ficha %>
  </div>
  <div class="field">
    <%= f.label :tiene_alguna_lesion?%><br>
    <%= f.check_box :lesion %>
  </div>
  <div class="actions">
    <%= f.submit  "Guardar"%>
  </div>
<% end %>
