<h1>Listing nombrepartidots</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nombrepartidots.each do |nombrepartidot| %>
      <tr>
        <td><%= nombrepartidot.nombre %></td>
        <td><%= link_to 'Show', nombrepartidot %></td>
        <td><%= link_to 'Edit', edit_nombrepartidot_path(nombrepartidot) %></td>
        <td><%= link_to 'Destroy', nombrepartidot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nombrepartidot', new_nombrepartidot_path %>
