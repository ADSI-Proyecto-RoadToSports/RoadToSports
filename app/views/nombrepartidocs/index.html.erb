<h1>Listing nombrepartidocs</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nombrepartidocs.each do |nombrepartidoc| %>
      <tr>
        <td><%= nombrepartidoc.nombre %></td>
        <td><%= link_to 'Show', nombrepartidoc %></td>
        <td><%= link_to 'Edit', edit_nombrepartidoc_path(nombrepartidoc) %></td>
        <td><%= link_to 'Destroy', nombrepartidoc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nombrepartidoc', new_nombrepartidoc_path %>
