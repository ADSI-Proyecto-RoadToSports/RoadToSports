  button_to 'Nuevo partido', new_pmicrofutbol_path, remote: true, :method => "get", class: "btn btn-info"
  end %>
<div class="paginacion">Partidos: <b><%= @pmicrofutbols.count %> </b><%= will_paginate @pmicrofutbols, :previous_label => '&laquo; Anterior', :next_label => 'Siguiente &raquo;' %></div>
</div>
