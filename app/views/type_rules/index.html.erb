<h1>Listing type_rules</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_rules.each do |type_rule| %>
      <tr>
        <td><%= type_rule.name %></td>
        <td><%= type_rule.description %></td>
        <td><%= link_to 'Show', type_rule %></td>
        <td><%= link_to 'Edit', edit_type_rule_path(type_rule) %></td>
        <td><%= link_to 'Destroy', type_rule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type rule', new_type_rule_path %>
