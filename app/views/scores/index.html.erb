<h1>Puntuaciónes</h1>
<%= form_tag scores_path, :method => 'get' do %>
<% content_tag :label do %>
<%= text_field_tag :search, params[:search] %>
<%= submit_tag "Buscar", :name => nil %>
<% end %>
<% end %>
<table  class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>Descripción</th>
      <th>Datescore</th>
      <th>Modalidad</th>
      <th>Equipo</th>
      <th colspan="3">Opciones</th>
    </tr>
  </thead>

  <tbody>
    <%= render @scores %>
  </tbody>
</table>

<br>

<%= button_to 'Nuevo puntuacion', new_score_path, remote: true, :method=>"get", class: "btn btn-info" %><br>
<%= will_paginate @scores, :previous_label => '&laquo; Anterior', :next_label => 'Siguiente 
&raquo;' %> 
<br> 
