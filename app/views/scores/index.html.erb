<div class="contenido ">
<div class="img1"></div>
<div class="img2"></div>
<ul class="nav nav-tabs" style="margin-bottom: 15px;">

</ul>
<div id="myTabContent" class="tab-content">
  <div class="tab-pane fade active in cont1" id="inicio">
<h1>Puntuación</h1>

<table>
  <thead>
    <tr>
      <th>Descripción</th>
      <th>Datescore</th>
      <th>Modalidad</th>
      <th>Equipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scores.each do |score| %>
      <tr>
        <td><%= score.description %></td>
        <td><%= score.datescore %></td>
        <td><%= score.modalitie %></td>
        <td><%= score.team %></td>
        <td><%= link_to 'Ver', score %></td>
        <td><%= link_to 'Editar', edit_score_path(score) %></td>
        <td><%= link_to 'Eliminar', score, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo puntuacion', new_score_path %><br>
<%= will_paginate @scores, :previous_label => '&laquo; Anterior', :next_label => 'Siguiente 
&raquo;' %> 
<br> 
Cantidad : <%= @scores.count %>
</div></div></div>