<div class="contenido ">
<div class="img1"></div>
<div class="img2"></div>
<ul class="nav nav-tabs" style="margin-bottom: 15px;">

</ul>
<div id="myTabContent" class="tab-content">
  <div class="tab-pane fade active in cont1" id="inicio">
<h1>Listing acceptances</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>User</th>
      <th>Rol</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @acceptances.each do |acceptance| %>
      <tr>
        <td><%= acceptance.description %></td>
        <td><%= acceptance.user %></td>
        <td><%= acceptance.rol %></td>
        <td><%= link_to 'Show', acceptance %></td>
        <td><%= link_to 'Edit', edit_acceptance_path(acceptance) %></td>
        <td><%= link_to 'Destroy', acceptance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Acceptance', new_acceptance_path %>
