<div id="myTabContent" class="tab-content">
  <div class="tab-pane fade active in cont1" id="inicio">
<h1>Listing pre_registrations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Modalities</th>
      <th>Description</th>
      <th>User</th>
      <th>Tournament</th>
      <th>Acceptance</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pre_registrations.each do |pre_registration| %>
      <tr>
        <td><%= pre_registration.name %></td>
        <td><%= pre_registration.modalities %></td>
        <td><%= pre_registration.description %></td>
        <td><%= pre_registration.user %></td>
        <td><%= pre_registration.tournament %></td>
        <td><%= pre_registration.acceptance %></td>
        <td><%= link_to 'Show', pre_registration %></td>
        <td><%= link_to 'Edit', edit_pre_registration_path(pre_registration) %></td>
        <td><%= link_to 'Destroy', pre_registration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pre registration', new_pre_registration_path %>
