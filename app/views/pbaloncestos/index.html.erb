<div class="contenidop ">
  <div class="img1"></div>
  <div class="img2"></div>
    <ul class="nav nav-tabs" style="margin-bottom: 15px;">
    </ul>
  <div id="myTabContent" class="tab-content">
    <div class="" id="inicio">
    <div class="slideExpandUp">
      <%= render 'nombrepartidobs/index' %>
      </div>
      <h1>Partidos de Baloncesto</h1>
        <div class="paginacion">Partidos: <b><%= @pbaloncestos.count %> </b><%= will_paginate @pbaloncestos, :previous_label => '&laquo; Anterior', :next_label => 'Siguiente &raquo;' %></div>
      <table class="table table-striped table-hover ">
        <thead>
          <tr>
            <th>Equipo1</th>
            <th  colspan="2">Encestas</th>
            <th>Puntos_Torneo</th>
            <th>Equipo2</th>
            <th  colspan="2">Encestas</th>
            <th>Puntos_Torneo</th>
            <th>Responsable</th>
            <th colspan="3">Opciones</th>
          </tr>
        </thead>

        <tbody>
         <%= render @pbaloncestos %>
        </tbody>
      </table>
      <br>
      <%= if can? :create, @pbaloncesto 
        button_to 'Nuevo partido', new_pbaloncesto_path, :method => "get", class: "btn btn-info", remote: true 
      end %>
      <div class="paginacion">Partidos: <b><%= @pbaloncestos.count %> </b><%= will_paginate @pbaloncestos, :previous_label => '&laquo; Anterior', :next_label => 'Siguiente &raquo;' %></div>
    </div>
    <p><%= link_to 'Generar(PDF)', pbaloncestos_path(@pbaloncestos, format: "pdf")%></p>
  </div>
</div>