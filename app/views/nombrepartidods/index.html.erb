<h1>Listing nombrepartidods</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nombrepartidods.each do |nombrepartidod| %>
      <tr>
        <td><%= nombrepartidod.nombre %></td>
        <td><%= link_to 'Show', nombrepartidod %></td>
        <td><%= link_to 'Edit', edit_nombrepartidod_path(nombrepartidod) %></td>
        <td><%= link_to 'Destroy', nombrepartidod, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nombrepartidod', new_nombrepartidod_path %>
