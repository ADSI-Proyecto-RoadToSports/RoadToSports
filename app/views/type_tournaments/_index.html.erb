<h1>Listing type_tournaments</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_tournaments.each do |type_tournament| %>
      <tr>
        <td><%= type_tournament.name %></td>
        <td><%= type_tournament.description %></td>
        <td><%= link_to 'Show', type_tournament %></td>
        <td><%= link_to 'Edit', edit_type_tournament_path(type_tournament) %></td>
        <td><%= link_to 'Destroy', type_tournament, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type tournament', new_type_tournament_path %>
