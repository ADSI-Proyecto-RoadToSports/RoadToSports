<div class="pajedrez">
<p id="notice"><%= notice %></p>

<p>
  <strong>Primer Participante:</strong>
  <%= @pajedrez.participante1 %>
</p>

<p>
  <strong>Puntos Participante1:</strong>
  <%= @pajedrez.puntos1 %>
  <%= if @pajedrez.gano1 == true and @pajedrez.gano2 == true
                em()
            else
                gan(@pajedrez.gano1) 
            end
   %>
</p>
<hr>
<p>
  <strong>Segundo Participante:</strong>
  <%= @pajedrez.participante2 %>
</p>

<p>
  <strong>Puntos Participante2:</strong>
  <%= @pajedrez.puntos2 %>
  <%= if @pajedrez.gano1 == true and @pajedrez.gano2 == true
                em()
            else
                gan(@pajedrez.gano2) 
            end
  %>
</p>
<hr>
<p>
  <strong>Torneo:</strong>
  <%= @pajedrez.tournament.name %>
</p>

<p>
  <strong>Fecha Del Torneo:</strong>
  <%= @pajedrez.fecha %>
</p>

<p>
  <strong>Estado:</strong>
  <%= estado(@pajedrez.estado) %>
</p>

<p>
  <strong>Responsable:</strong>
  <%= @pajedrez.user.nombre %>
</p>
</div>